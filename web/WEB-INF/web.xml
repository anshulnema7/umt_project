<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>GetReport</servlet-name>
        <servlet-class>com.test.GetReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IdeaInbox</servlet-name>
        <servlet-class>com.test.IdeaInbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DrInboxInbox</servlet-name>
        <servlet-class>com.test.DrInboxInbox</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.test.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetReport</servlet-name>
        <url-pattern>/GetReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IdeaInbox</servlet-name>
        <url-pattern>/IdeaInbox</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DrInboxInbox</servlet-name>
        <url-pattern>/DrInboxInbox</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
